<h1>New Booking</h1>
<div class="row justify-content-center">
  <div class="container col-4 m-3">
    <%= simple_form_for [@outfit, @booking]  do |f| %>
      <%= f.input :start_date, html5: true %>
      <%= f.input :end_date, html5: true %>
      <%= f.submit "Submit booking", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
